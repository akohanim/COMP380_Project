<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<Pane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CourseTile">
   <children>
      <VBox prefHeight="200.0" prefWidth="200.0" style="-fx-border-color: black;">
         <children>
            <Button fx:id="classButton" alignment="CENTER" minHeight="150.0" minWidth="200.0" mnemonicParsing="false" onAction="#clickClassButton">
               <padding>
                  <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
               </padding>
            </Button>
            <HBox alignment="CENTER" minHeight="5.0" minWidth="200.0" spacing="10.0">
               <children>
                  <Label fx:id="classNameLabel" textAlignment="JUSTIFY" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#clickSettings" text="Settings" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</Pane>
